	/*
	 * POST NAVIGATION
	 */

	.nav-links:after {
		visibility: hidden;
		display: block;
		font-size: 0;
		content: " ";
		clear: both;
		height: 0;
	}

	.nav-links {
		display:lock;
	}

	.navigation .nav-previous{
		float:left;
	}

	.navigation .nav-previous:before {
		content: "←";
		margin-right: 5px;
		font-size: 20px;
		color: #6B9F50;
		line-height: 14px;
	}

	.navigation .nav-next{
		float:right;
	}

	.navigation .nav-next:after{
		content: "→";
		margin-left: 5px;
		font-size: 20px;
		color: #6B9F50;
		line-height: 14px;
	}


/*======================================
    Media
========================================*/
.page-content img.wp-smiley,
.entry-content img.wp-smiley,
.comment-content img.wp-smiley {
	border: none;
	margin-bottom: 0;
	margin-top: 0;
	padding: 0;
}
.wp-caption {
	border: 1px solid #ccc;
	margin-bottom: 1.5em;
	max-width: 100%;
}
.wp-caption img[class*="wp-image-"] {
	display: block;
	margin: 1.2% auto 0;
	max-width: 98%;
}
.wp-caption-text {
	text-align: center;
}
.wp-caption .wp-caption-text {
	margin: 0.8075em 0;
}
.site-main .gallery {
	margin-bottom: 1.5em;
}
.gallery-caption {
}
