	body,
	html{
		font-family: 'Open Sans', sans-serif !important;
		color: #2f342c;
	}

	#page{
		position: relative;
	}

	h1{
		font-size:40px ;
	}

	h2{
		font-size:20px;
	}

	h3{
		font-size: 15px;
	}

	h4, h5, h6{
		font-size:20px;
	}

	p{
		line-height: 35px;
	}

	a,
	a:visited,
	a:link{
		/*color: #5d963f;*/
